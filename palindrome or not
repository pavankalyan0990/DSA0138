#include<iostream>
using namespace std;
int main()
{
  int num=121,reversed=0,rem,originalnum;
  originalnum=num;
  while(num!=0)
  {
    rem=num%10;
    reversed=reversed*10+rem;
    num/=10;
  }
  if(originalnum=reversed)
  {
    cout<<originalnum<<" is a palindrome"<<endl;
  }
  else
  {
    cout<<originalnum<<" isnot a palindrome"<<endl;
  }
  return 0;
  }
