#include<iostream>
using namespace std;
int main()
{
  int year=2024,month=4,date=10;
  if(year %400==0)
  {
    cout<<date<<"/"<<month<<"/"<<year<<"is a leap year"<<endl;
  }
  else if(year%100==0)
  {
    cout<<date<<"/"<<month<<"/"<<year<<"is not a leap year"<<endl;
  }
  else if(year%4==0)
  {
     cout<<date<<"/"<<month<<"/"<<year<<"is not a leap year"<<endl;
  }
  return 0;
  }
