#include<iostream>
using namespace std;
int main()
{
	int a[2][2]={8,7,6,5};
	int b[2][2]={4,3,2,1};
	int c[2][2]={};
	int i,j;
	for(i=0;i<2;i++)
	{
		for(j=0;j<2;j++)
		{
			c[i][j]=a[i][j]-b[i][j];
		}
	}
	for(i=0;i<2;i++)
	{
		for(j=0;j<2;j++)
		{
			cout<<c[i][j]<<" ";
		}
		cout<<endl;
	}
	return 0;
}
